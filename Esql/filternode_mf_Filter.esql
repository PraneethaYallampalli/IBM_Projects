

CREATE FILTER MODULE filternode_mf_Filter
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		
		DECLARE inRef REFERENCE TO Root.XMLNSC.[<]; 
		IF FIELDNAME (Root.XMLNSC.[<] )='employees' THEN
			RETURN TRUE;
		ELSEIF  FIELDNAME (Root.XMLNSC.[<]) ='student' THEN
			RETURN FALSE;
		ELSE
			RETURN UNKNOWN;
		END IF;
		RETURN TRUE;
	END;

END MODULE;
